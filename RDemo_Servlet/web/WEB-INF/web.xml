<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <description>This is a first demo servlet project</description>
    <context-param>
        <param-name>Semester</param-name>
        <param-value>6</param-value>
    </context-param>
    <context-param>
        <param-name>PhoneNumber</param-name>
        <param-value>9825098250</param-value>
    </context-param>
    <filter>
        <filter-name>Filter1</filter-name>
        <filter-class>DemoPack.Filter1</filter-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Filter1</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>Filter1</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>Filter2</filter-name>
        <filter-class>DemoPack.Filter2</filter-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Filter2</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>Filter2</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>DemoPack.Servlet1</servlet-class>
        <init-param>
            <param-name>Rollno</param-name>
            <param-value>abc123</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Servlet3</servlet-name>
        <servlet-class>DemoPack.Servlet3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AS_Servlet_1</servlet-name>
        <servlet-class>Attributes_Session.AS_Servlet_1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AS_Servlet_2</servlet-name>
        <servlet-class>Attributes_Session.AS_Servlet_2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/formdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet3</servlet-name>
        <url-pattern>/Servlet3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AS_Servlet_1</servlet-name>
        <url-pattern>/AS_Servlet_1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AS_Servlet_2</servlet-name>
        <url-pattern>/AS_Servlet_2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>50</session-timeout>
    </session-config>
 
</web-app>
